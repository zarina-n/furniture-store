{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          }
        ]
      },
      {
        "source": "/_next/image/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600, immutable"
          }
        ]
      }
    ],
    "frameworksBackend": {
      "region": "us-east1"
    },
    "redirects": [
      {
        "source": "/**",
        "destination": "https://theinterior.site",
        "type": 301
      }
    ]
  }
}
